{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2"
  },
  "icon_theme": "Material Icon Theme",
  "features": {
    "edit_prediction_provider": "zed"
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "soft_wrap": "editor_width",
  "vim_mode": true,
  "ui_font_size": 18,
  "buffer_font_family": "FiraCode Nerd Font Mono",
  "buffer_font_size": 18,
  "buffer_font_features": {
    "ligatures": true
  },
  "restore_on_startup": "none",
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "confirm_quit": true,
  "load_direnv": "direct",
  "theme": {
    "mode": "system",
    "light": "Catppuccin Mocha",
    "dark": "Catppuccin Mocha"
  },
  "auto_update": false
}
